<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baobaotao.web.dao.UserDao">
	<select
		id="getUsers"
		resultType="HashMap">
		SELECT
		*
		FROM
		t_user
	</select>
	<select
		id="getUserById"
		parameterType="HashMap"
		resultType="HashMap">
		SELECT
		*
		FROM
		t_user
		WHERE id = #{id}
	</select>
</mapper>
